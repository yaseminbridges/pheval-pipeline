params {
  data_dir        = "/data/exomiser"
  config_template = "configs/config_template.yml"

  exomiser_configs = [
    [ exomiser_version: "14.1.0", phenotype_db: "2508", hg19_db: "2508", hg38_db: "2508", preset: "preset-exome-analysis.yml" ],
    [ exomiser_version: "14.1.0", phenotype_db: "2508", hg19_db: "2508", hg38_db: "2508", preset: "preset-exome-analysis_human_only.yml" ]
  ]
}

profiles {
  local {
    includeConfig 'conf/local.config'
  }
  hpc {
    includeConfig 'conf/hpc.config'
  }
}